        container _stateSpec {
            config true;
            uses TapiModule-ObjectClasses-StatePac:G_Tapi_AdminStatePac;
        }
        container _transferCapacitySpec {
            config true;
            uses G_Tapi_TransferCapacityPac;
        }
        container _transferCostSpec {
            config true;
            uses G_Tapi_TransferCostPac;
        }
        container _transferIntegritySpec {
            config true;
            uses G_Tapi_TransferIntegrityPac;
        }
        container _transferTimingSpec {
            config true;
            uses G_Tapi_TransferTimingPac;
        }
        leaf-list layerProtocolName {
            ordered-by system;
            type TapiModule-TypeDefinitions:Tapi_LayerProtocolName;
            min-elements 1;
        }
    }
    grouping G_Tapi_Link {
        uses G_Tapi_GlobalComponent;
        list _linkPort {
            key 'localId';
            config true;
            ordered-by system;
            uses G_Tapi_LinkPort;
            min-elements 2;
        }
        leaf-list _node {
            ordered-by system;
            type leafref {
                path '/Tapi_Topology/_node/uuid';
            }
            min-elements 2;
        }
        container _stateSpec {
            config true;
            uses TapiModule-ObjectClasses-StatePac:G_Tapi_AdminStatePac;
        }
        container _transferCapacitySpec {
            config true;
            uses G_Tapi_TransferCapacityPac;
        }
        container _transferCostSpec {
            config true;
            uses G_Tapi_TransferCostPac;
        }
        container _transferIntegritySpec {
            config true;
            uses G_Tapi_TransferIntegrityPac;
        }
        container _transferTimingSpec {
            config true;
            uses G_Tapi_TransferTimingPac;
        }
        container _riskParameterSpec {
            config true;
            uses G_Tapi_RiskParameterPac;
        }
        container _validationSpec {
            config true;
            uses G_Tapi_ValidationPac;
        }
        container _lpTransitionSpec {
            config true;
            uses G_Tapi_LayerProtocolTransitionPac;
        }
        leaf-list layerProtocolName {
            ordered-by system;
            type TapiModule-TypeDefinitions:Tapi_LayerProtocolName;
            min-elements 1;
        }
        leaf direction {
            type TapiModule-TypeDefinitions:Tapi_ForwardingDirection;
            description "The directionality of the Link. 
                Is applicable to simple Links where all LinkEnds are BIDIRECTIONAL (the Link will be BIDIRECTIONAL) or UNIDIRECTIONAL (the Link will be UNIDIRECTIONAL). 
                Is not present in more complex cases.";
        }
    }
