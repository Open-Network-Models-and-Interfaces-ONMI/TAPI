             |  |  +--ro value-name    string
             |  |  +--ro value?        string
             |  +--ro administrative-state?                 administrative-state
             |  +--ro operational-state?                    operational-state
             |  +--ro lifecycle-state?                      lifecycle-state
             |  +--ro total-potential-capacity
