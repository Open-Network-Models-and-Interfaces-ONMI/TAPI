             |     |  +--rw value-name    string
             |     |  +--rw value?        string
             |     +--rw oam-job?                          tapi-common:uuid
             |     +--rw pm-metric-info
             |     |  +--rw threshold-observed-value
             |     |  |  +--rw pm-parameter-int-value?    uint64
             |     |  |  +--rw pm-parameter-real-value?   decimal64
             |     |  +--rw threshold-configured-value
             |     |     +--rw pm-parameter-int-value?    uint64
             |     |     +--rw pm-parameter-real-value?   decimal64
             |     +--rw detector-info
             |     |  +--rw perceived-severity?   perceived-severity-type
             |     |  +--rw service-affecting?    service-affecting
             |     |  +--rw is-acknowledge?       boolean
             |     |  +--rw detector-category?    detector-category
